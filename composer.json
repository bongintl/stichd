{
  "require": {
    "craftcms/cms": "^3.0.0",
    "craftcms/redactor": "2.3.3.2",
    "ether/seo": "^3.5",
    "mikestecker/craft-videoembedder": "^1.1",
    "ostark/craft-async-queue": "^2.0",
    "verbb/field-manager": "^2.1",
    "vlucas/phpdotenv": "^2.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/",
      "bongTwigExtension\\": "modules/bong-twig-extension/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
